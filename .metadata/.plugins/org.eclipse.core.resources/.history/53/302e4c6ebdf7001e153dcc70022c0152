package com.Boot.demo.controller;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Component;

import com.Boot.demo.service.Honda;
import com.Boot.demo.service.Toyato;

@Component
public class CarController {
	
	private Toyato toyato;
	

	public CarController(Toyato toyato) {
		this.toyato=toyato;
	}
	
	
//	@Autowired
	private Honda honda;
	
	
	
	//-->Constructor Injection
	
	@Autowired
	public CarController(Honda honda) {
		this.honda=honda;
	}
	
	//-->Setter Injection
//	@Autowired
//	public void setHondaCar(Honda honda) {
//		this.honda=honda;
//	}
	
	
	public String getCar() {
		 return honda.getHonda() ;
	}
 
}
